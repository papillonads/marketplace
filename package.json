{
  "name": "marketplace",
  "description": "Marketplace",
  "homepage": "https://github.com/papillonads/marketplace/#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/papillonads/marketplace.git"
  },
  "scripts": {
    "clean-up": "rm -rf .storybook-static coverage node_modules package-lock.json && npm i lerna -S && lerna run clean-up --stream",
    "install-packages": "npm install && lerna run install-packages --stream",
    "make": "npm install --prod && npm rebuild node-sass",
    "make:core": "npm run make && lerna run make:core --stream",
    "make:docs": "npm run make && lerna run make:docs --stream",
    "install-papillonads-latest": "npm i \"@papillonads/components\"@latest && npm i \"@papillonads/library\"@latest",
    "bootstrap": "npm-run-all build build-storybook lint:format test report-coverage",
    "build": "lerna run build --stream",
    "build-storybook": "NODE_ENV=develop build-storybook -c .storybook -o .storybook-static",
    "lint:format": "npm-run-all lint format",
    "lint": "npm-run-all lint:js:jsx lint:scss",
    "lint:js:jsx": "eslint --ext .js --ext .jsx packages/**/src --fix",
    "lint:scss": "stylelint \"packages/**/src/**/*.{css,scss}\" --syntax=scss --fix",
    "format": "npm-run-all format:js:jsx format:scss",
    "format:js:jsx": "prettier --write \"packages/**/src/**/*.{js,jsx}\"",
    "format:scss": "prettier --write \"packages/**/src/**/*.{css,scss}\"",
    "test": "jest -u --silent --runInBand --coverage",
    "report-coverage": "codecov",
    "start-storybook": "NODE_ENV=develop start-storybook -p 7007 -c .storybook",
    "release": "lerna publish",
    "deploy-core": "gh-pages -d packages/core/build",
    "deploy": "npm-run-all build deploy-core"
  },
